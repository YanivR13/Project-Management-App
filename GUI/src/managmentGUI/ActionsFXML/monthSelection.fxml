<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="247.0" prefWidth="285.0" stylesheets="@../../clientGUI/cssStyle/GlobalStyles.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="managmentGUI.MonthSelectionController">

    <children>
        <Label layoutX="39.0" layoutY="25.0" text="Please choose month for report" />

        <ComboBox fx:id="monthCombo" layoutX="42.0" layoutY="58.0" prefWidth="200" promptText="Pick a month">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="January" />
                    <String fx:value="February" />
                    <String fx:value="March" />
                    <String fx:value="April" />
                    <String fx:value="May" />
                    <String fx:value="June" />
                    <String fx:value="July" />
                    <String fx:value="August" />
                    <String fx:value="September" />
                    <String fx:value="October" />
                    <String fx:value="November" />
                    <String fx:value="December" />
                </FXCollections>
            </items>
        </ComboBox>

        <Button fx:id="timeRepBtn" layoutX="78.0" layoutY="95.0" onAction="#generateTimeReports" style="-fx-background-color: green;" text="Time Reports" />
        <Button fx:id="btnback" layoutX="82.0" layoutY="186.0" onAction="#closeWin" prefHeight="34.0" prefWidth="120.0" style="-fx-background-color: red;" text="close" />
      <Button fx:id="subRepBtn" layoutX="82.0" layoutY="142.0" onAction="#generateSubscriberReports" style="-fx-background-color: green;" text="Sub Reports" />
    </children>

</AnchorPane>
